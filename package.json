{
  "type": "module",
  "name": "phi-components",
  "files": [
    "dist"
  ],
  "main": "./dist/phi-components.umd.js",
  "module": "./dist/phi-components.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/phi-components.es.js",
      "require": "./dist/phi-components.umd.js"
    }
  },
  "scripts": {
    "build": "npm run build:doc && npm run build:module",
    "build:module": "rm -r dist && tsc && vite build",
    "build:doc": "rm -r doc && vitepress build",
    "analyze": "vite build --mode analyze",
    "dev": "vitepress dev",
    "serve": "vitepress serve"
  },
  "devDependencies": {
    "@preact/signals": "^1.1.3",
    "goober": "2.1.x",
    "preact": "^10.7.0",
    "preactement": "zk-phi/component-elements#2ba7d8d31c7bee9eb47a7dbdb7e5354ba772fea3",
    "rollup-plugin-visualizer": "5.12.x",
    "typescript": "5.4.x",
    "vite": "5.3.x",
    "vitepress": "1.2.x"
  }
}
